import java.util.Random;

public class PhoneNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int [] i = new int[9];
		i[0]=6;
		i[1]=7;
		Random ran =new Random();
		for(int j=0;j<=9;j++) {
			int number=ran.nextInt(7);
			System.out.print(number);
		}
		
	}

}
