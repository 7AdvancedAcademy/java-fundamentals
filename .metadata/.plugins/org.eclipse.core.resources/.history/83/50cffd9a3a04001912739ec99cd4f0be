import java.util.Scanner;

public class TicTacToe {
	
	public static void main(String[] args) {
	System.out.println("Welcome on tic tac toe game");
	move();
	System.out.println("---------------");
	System.out.println("     |    |   	");
	System.out.println("---------------");
	System.out.println("     |    |   	");
	System.out.println("---------------");
	System.out.println("     |    |   	");
	System.out.println("---------------");
		
		
	} 
	public static String[][] move() {
		Scanner input = new Scanner(System.in);
		System.out.println("make your move");

		int fistChoice=input.nextInt();
		int SecondChoise=input.nextInt();
		String[][] i=new String[fistChoice][SecondChoise];
		return i;
	}
	
}
