		//calculating sum of numbers between 0 and a given number
			


		int max;
		

		Scanner input = new Scanner(System.in);
		
		System.out.println("Enter your max : ");
		max = input.nextInt();
		
		for(int i = 0;i < max;i++) {
			int sum = 0;
		}
		System.out.println("Sum of numbers between 1 and "+max+" is "+sum);
	}

}
