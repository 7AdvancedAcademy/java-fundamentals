	int[][] a= {{},{},{}};
	
	System.out.println("------------");
	System.out.println(" a[0][0]=X   |    |  ");
	System.out.println("------------");
	System.out.println("    |    |  ");
	System.out.println("------------");
	System.out.println("    |    |  ");
	System.out.println("------------");
		
		
	} 
	
}
